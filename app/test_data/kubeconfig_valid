clusters:
- name: cluster_1
  cluster:
    server: https://foo.com
    certificate-authority: ca1
- name: cluster_2
  cluster:
    server: https://bar.com
    certificate-authority: ca2
contexts:
- name: dev
  context:
    cluster: cluster_2
    namespace: red
    user: user_2
- name: prod
  context:
    cluster: cluster_1
    namespace: blue
    user: user_1
current-context: prod
users:
- name: user_1
  user:
    client-certificate: cert1
    client-key: key1
- name: user_2
  user:
    client-certificate: cert2
    client-key: key2
